# Cloudflare R2 Credentials
NEXT_PUBLIC_R2_CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
NEXT_PUBLIC_R2_ACCESS_KEY_ID=your-cloudflare-access-key-id
NEXT_PUBLIC_R2_SECRET_ACCESS_KEY=your-cloudflare-secret-access-key
NEXT_PUBLIC_R2_BUCKET_NAME=your-r2-bucket-name

# Blockchain/RPC Connection
NEXT_PUBLIC_CHAIN_RPC=https://your-rpc-url.example.com

# Blockchain Private Key (NEVER commit the real one)
NEXT_PUBLIC_PRIVATE_KEY=your-private-key

# IPFS Space Identifier or URL
NEXT_PUBLIC_IPFS_SPACE=your-ipfs-space-id-or-url
